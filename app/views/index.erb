
<div class="event_container">
  <h1>Upcoming Events</h1>
  <table>
  <tr>
    <th>Date</th>
    <th>Event</th>
  </tr>
  <% @events.each do |event| %>
  <tr>
    <td><%= event.date %></td>
    <td><a href="/events/<%=event.id%>/show"><%= event.title %></a></td>
  </tr>
  <% end %>
  </table>
</div>
